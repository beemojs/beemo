{
  "name": "@beemo/driver-stylelint",
  "version": "2.0.0-rc.1",
  "release": "1614239076609",
  "description": "Stylelint driver for Beemo.",
  "keywords": [
    "beemo",
    "beemo-driver",
    "driver",
    "stylelint",
    "lint",
    "linter"
  ],
  "scripts": {
    "integration:cli": "stylelint --invalid-option",
    "integration:fail": "stylelint --config ./integration/.stylelintrc.js ./integration/fail.css ./integration/fail-syntax.css",
    "integration:pass": "stylelint --config ./integration/.stylelintrc.js ./integration/pass.css"
  },
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:beemojs/beemo.git",
    "directory": "packages/driver-stylelint"
  },
  "author": "Miles Johnson",
  "contributors": [
    "Michael Hayes"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "stylelint": "^13.13.0"
  },
  "peerDependencies": {
    "@beemo/core": "^2.0.0-alpha.0",
    "stylelint": "^13.0.0"
  },
  "dependencies": {
    "@boost/event": "^2.3.2",
    "@types/stylelint": "*"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "platform": "node"
  }
}
