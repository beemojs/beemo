{
  "name": "@beemo/driver-eslint",
  "version": "2.0.0-rc.1",
  "release": "1614239076609",
  "description": "ESLint driver for Beemo.",
  "keywords": [
    "beemo",
    "beemo-driver",
    "driver",
    "eslint",
    "lint",
    "linter"
  ],
  "scripts": {
    "integration:cli": "eslint --no-eslintrc --invalid-option",
    "integration:fail": "eslint --no-eslintrc ./integration/fail.js ./integration/fail-syntax.js",
    "integration:pass": "eslint --no-eslintrc ./integration/pass.js"
  },
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:beemojs/beemo.git",
    "directory": "packages/driver-eslint"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "eslint": "^7.25.0"
  },
  "peerDependencies": {
    "@beemo/core": "^2.0.0-alpha.0",
    "eslint": "^7.0.0"
  },
  "dependencies": {
    "@boost/event": "^2.3.2",
    "@types/eslint": "*"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "platform": "node"
  }
}
